{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "matchDatasources": ["docker"],
      "matchPackageNames": ["ghcr.io/linuxserver/calibre-web"],
      "allowedVersions": "<1"
    },
    {
      "matchDatasources": ["docker"],
      "matchPackagePatterns": ["postgresql"],
      "allowedVersions": "<17"
    },
    {
      "matchDatasources": ["docker"],
      "matchPackagePatterns": ["node"],
      "allowedVersions": "<20"
    }
  ]
}
